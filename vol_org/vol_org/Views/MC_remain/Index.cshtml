        </th>
        <th>
            @Html.DisplayNameFor(model => model.MC.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.amount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MC.name)
        </td>
        <td>
